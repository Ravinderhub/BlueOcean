pipeline {
  agent any
  stages {
    stage('TEST') {
      steps {
        echo 'HELLO ITS A TEST'
      }
    }

    stage('BUILD FOLDER') {
      steps {
        echo 'ITS THE BUILD PART'
      }
    }

    stage('DEPLOY') {
      steps {
        echo 'ITS THE DEPOLYMENT PART'
      }
    }

  }
}